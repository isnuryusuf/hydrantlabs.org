	│   │   │   └── office_space.theme.mp3
	│   │   ├── Pictures
	│   │   └── Public
	│   │       └── taxes.txt
	│   ├── guardbot
	│   │   ├── Desktop
	│   │   ├── Documents
	│   │   │   └── log
